int foo(int x) {
    for int i = 10 downto x do {
        if (i <= 8) {
            return i;
        }
    }
    return 0;
}

int foo2(int x) {
    while (x >= 0) {
        if (x <= 5) {
            return x;
        }
        x = x - 1;
    }
    return 0;
}

int main() {
    print foo(2);
    print foo2(20);
    return 0;
}