# Recursion

int fib (int n) {
    if (n == 0) {
        return 0;
    }

    if (n == 1) {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}

int factor(int n) {
    if (n == 1) {
        return 1;
    }
    return factor(n-1) * n; 
}

int main() {
    print fib(7);
    print factor(5);
    return 0;
}